{
  "name": "resume-starter",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@7.1.0",
  "scripts": {
    "start": "vite serve --port 8080",
    "build": "vite build",
    "preview": "vite preview --port 8080",
    "check:lint": "eslint \"**/*.{ts,tsx}\"",
    "check:ts": "tsc --noEmit",
    "check:deps": "ncu -u",
    "check:ts:lint": "npm run check:ts && npm run check:lint",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{json,html,md,yaml}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "npm run check:ts:lint",
      "git add"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-syntax-flow": "^7.23.3",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@commitlint/cli": "^19.1.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@testing-library/react": "^14.2.1",
    "@tool-pack/basic": "^0.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.26",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "npm-check-updates": "^16.14.15",
    "prettier": "^3.2.5",
    "react-scripts": "^5.0.1",
    "resetcss": "^1.0.3",
    "sass": "^1.71.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "resume starter",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mengxinssfd/resume-starter.git"
  },
  "author": "DYH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mengxinssfd/resume-starter/issues"
  }
}
