{
  "name": "@mxssfd/resume-starter",
  "version": "0.2.1",
  "packageManager": "pnpm@9.3.0",
  "scripts": {
    "start": "vite serve --port 8080",
    "build": "vite build",
    "build:lib": "pnpm build:lib:vite && pnpm build:lib:dts",
    "build:lib:vite": "vite build -c lib.vite.config.ts",
    "analyze:lib": "vite build -c lib.vite.config.ts --mode analyze",
    "build:lib:dts": "tsc -p ./tsconfig.build.json --emitDeclarationOnly && tsc-alias -p ./tsconfig.build.json && rollup -c ./dts.rollup.config.js",
    "preview": "vite preview --port 8080",
    "check:lint": "eslint \"**/*.{ts,tsx}\"",
    "check:ts": "tsc --noEmit",
    "check:all": "pnpm check:ts & pnpm check:lint",
    "check:deps": "ncu -u",
    "check:ts:lint": "npm run check:ts && npm run check:lint",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "release": "esno scripts/release.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "node": "./dist/index.es.js",
        "default": "./dist/index.es.js",
        "types": "./dist/index.d.ts"
      },
      "require": {
        "default": "./dist/index.es.js",
        "types": "./dist/index.d.ts"
      }
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "lint-staged": {
    "*.{json,html,md,yaml}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "npm run check:ts:lint",
      "git add"
    ]
  },
  "dependencies": {
    "@tool-pack/basic": "^0.7.0",
    "@tool-pack/dom": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "markdown-it": "^14.1.0",
    "pdf-lib": "^1.17.1"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-syntax-flow": "^7.23.3",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@commitlint/cli": "^19.1.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@react-buddy/ide-toolbox": "^2.4.0",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^22.5.2",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@types/semver": "^7.5.8",
    "@vitejs/plugin-react": "^4.2.1",
    "chalk": "^5.3.0",
    "conventional-changelog-cli": "^4.1.0",
    "enquirer": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "esno": "^4.7.0",
    "execa": "^8.0.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "npm-check-updates": "^16.14.15",
    "prettier": "^3.2.5",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "resetcss": "^1.0.3",
    "rollup": "^4.13.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.71.1",
    "semver": "^7.6.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "web-vitals": "^3.5.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "resume starter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mengxinssfd/resume-starter.git"
  },
  "author": "DYH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mengxinssfd/resume-starter/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
